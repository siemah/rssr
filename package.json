    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/helmet": "^0.0.47",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-router-dom": "^5.1.5",
    "@types/serialize-javascript": "^4.0.0",
    "@types/validator": "^13.1.0",
    "babel-loader": "^8.1.0",
